/****************************************/
/*****		    QCheckBox  			*****/
/****************************************/

QCheckBox#infoCheckbox
{
	font-size	: 30px;
	color		: Yellow;
}

QCheckBox::indicator#infoCheckbox {
	width		: 50px;
	height		: 50px;
}

QCheckBox::indicator:checked#infoCheckbox {
	image		: url(:/imgs/checkbox_checked_yellow.png);
}

QCheckBox::indicator:unchecked#infoCheckbox {
	image		: url(:/imgs/checkbox_unchecked_yellow.png);
}

QCheckBox#quizCreatorCheckbox
{
	font-size	: 18px;
}

QCheckBox::indicator#quizCreatorCheckbox {
	width		: 25px;
	height		: 25px;
}

QCheckBox::indicator:checked#quizCreatorCheckbox {
	image		: url(:/imgs/checkbox_checked_black.png);
}

QCheckBox::indicator:unchecked#quizCreatorCheckbox {
	image		: url(:/imgs/checkbox_unchecked_black.png);
}


QCheckBox#settingsCheckbox
{
	font-size	: 22px;
	color		: Yellow;
}

QCheckBox#settingsCheckbox:disabled
{
	color		: rgb(100, 100, 0);
}

QCheckBox::indicator#settingsCheckbox {
	width		: 30px;
	height		: 30px;
}

QCheckBox::indicator:checked#settingsCheckbox {
	image		: url(:/imgs/checkbox_checked_yellow.png);
}

QCheckBox::indicator:unchecked#settingsCheckbox {
	image		: url(:/imgs/checkbox_unchecked_yellow.png);
}

QCheckBox::indicator:checked#settingsCheckbox:disabled {
	image		: url(:/imgs/checkbox_checked_yellow_disabled.png);
}

QCheckBox::indicator:unchecked#settingsCheckbox:disabled {
	image		: url(:/imgs/checkbox_unchecked_yellow_disabled.png);
}


