}



/****************************************/
/*****			 QSlider			*****/
/****************************************/

QSlider::handle:horizontal#settingsSlider
{
	background-color		: Yellow;
}

QSlider::groove:horizontal#settingsSlider {
    height					: 7px;
    margin					: 2px 0;
    border					: 1px solid #999999;
    background				 qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #B1B1B1, stop:1 #c4c4c4);
}

QSlider::handle:horizontal#settingsSlider {
    width					: 28px;
    margin					: -7px 0;
    border-radius			: 4px;
    border					: 1px solid #5c5c5c;
    background				: qlineargradient(x1:0, y1:0, x2:1, y2:1, stop:0 #b4b4b4, stop:1 #8f8f8f);
}

QSlider::sub-page:horizontal#settingsSlider {
    height					: 7px;
    background				: rgb(230, 230, 0);
}

QSlider::add-page:horizontal#settingsSlider {
    height					: 7px;
    background				: Lightgrey;
}

QSlider::sub-page:horizontal#settingsSlider:disabled {
    background				: Grey;
}

QSlider::add-page:horizontal#settingsSlider:disabled {
    background				: Grey;
}