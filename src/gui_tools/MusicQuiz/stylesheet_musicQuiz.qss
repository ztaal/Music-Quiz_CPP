/****************************************/
/*****		    QPushButton			*****/
/****************************************/

QPushButton[objectName^="QuizEntry"]
{
	color				: Yellow;
	font-size			: 40px;

	background-color	: rgb(0, 0, 255);

	border-color		: rgb(0, 0, 255);
	border-radius		: 0px;
	border-width		: 1px;
	border-style		: solid;
	margin				: 0px;
	padding				: 0px;

	min-height			: 100px;
	min-width			: 200px;
}

QPushButton[objectName^="TeamEntry"]
{
	font-size			: 40px;
	font-weight			: bold;

	min-height			: 100px;
	min-width			: 200px;
}

